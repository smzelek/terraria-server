{
        "agent": {
                "metrics_collection_interval": 60,
                "run_as_user": "root",
                "omit_hostname": true
        },
        "metrics": {
                "metrics_collected": {
                        "disk": {
                                "drop_device": true,
                                "measurement": [
                                        "used_percent"
                                ],
                                "metrics_collection_interval": 60,
                                "resources": [
                                        "/"
                                ],
                                "append_dimensions": {
                                        "Resource": "terraria-server"
                                }
                        },
                        "mem": {
                                "measurement": [
                                        "mem_used_percent"
                                ],
                                "metrics_collection_interval": 30,
                                "append_dimensions": {
                                        "Resource": "terraria-server"
                                }
                        }
                }
        }
}